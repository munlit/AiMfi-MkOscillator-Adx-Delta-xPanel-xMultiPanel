		'3'								=> '3 min'
		'5'								=> '5 min'
		'15'							=> '15 min'
		'30'							=> '30 min'
		'60'							=> '1 Hora'
		'120'							=> '2 Horas'
		'180'							=> '3 Horas'
		'240'							=> '4 Horas'
		'480'							=> '8 horas'
		'D'								=> 'Dia'
		'1D'							=> 'Dia'
		'W'								=> 'Semana'
